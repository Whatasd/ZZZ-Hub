local DiscordLib =
    loadstring(game:HttpGet "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord")()

local win = DiscordLib:Window("made by discord.gg/vCR9sezB")

local serv = win:Server("ZZZ Hub", "")


--channels
local geode = serv:Channel("AutoSpawnBlocks bypass")



--geode
geode:Toggle(
    "Auto SpawnLuckyBlock Yellow (op)",
    false,
    function(bool)
        getgenv().AutoClicker = true
while AutoClicker do
    wait()
    if bool then
        wait()
game:GetService("ReplicatedStorage").SpawnLuckyBlock:FireServer()
    else
        getgenv().AutoClicker = false
    end
    end
    end
)


